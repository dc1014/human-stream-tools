{
    "rules": {
        "quotes": [2, "single"],
        "comma-dangle": [2, "always-multiline"],
        "no-cond-assign": [2, "always"],
        "no-constant-condition": 2,
        "no-console": 0,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 2,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": 2,
        "valid-typeof": 2,
        "block-scoped-var": 2,
        "curly": [2, "multi"],
        "default-case": 2,
        "dot-location": [2, "property"],
        "dot-notation": [2, {"allowKeywords": true}],
        "eqeqeq": 2,
        "no-arrow-condition": 0,
        "no-else-return": 2,
        "no-empty-label": 2,
        "no-empty-pattern": 2,
        "no-eq-null": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": 2,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-magic-numbers": 2,
        "no-multi-spaces": 2,
        "semi": [2, "never"]
    },
    "ecmaFeatures": {
        "arrow-body-style": [2, "as-needed"],
        "arrow-parens": [2, "as-needed"],
        "arrowFunctions": 2,
        "blockBindings": 2,
        "defaultParams": 2,
        "destructuring": 2,
        "forOf": 2,
        "generators": 2,
        "jsx": 2,
        "modules": 2,
        "no-var": 2,
        "objectLiteralComputedProperties": 2,
        "objectLiteralDuplicateProperties": 2,
        "objectLiteralShorthandMethods": 2,
        "objectLiteralShorthandProperties": 2,
        "restParams": 2,
        "spread": 2,
        "templateStrings": 2
    }
}
